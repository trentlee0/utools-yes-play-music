{
  "pluginName": "YesPlayMusic",
  "author": "Trent0",
  "homepage": "https://github.com/trentlee0",
  "description": "YesPlayMusic 的一些快捷操作",
  "version": "1.0.0",
  "logo": "logo.png",
  "preload": "preload.js",
  "platform": [
    "darwin"
  ],
  "features": [
    {
      "code": "music-play-pause",
      "explain": "YesPlayMusic 开始或暂停播放",
      "cmds": [
        "Play Pause",
        "开始或暂停播放"
      ]
    },
    {
      "code": "music-prev",
      "explain": "YesPlayMusic 上一首",
      "cmds": [
        "Previous",
        "上一首"
      ]
    },
    {
      "code": "music-next",
      "explain": "YesPlayMusic 下一首",
      "cmds": [
        "Next",
        "下一首"
      ]
    },
    {
      "code": "music-like",
      "explain": "YesPlayMusic 喜欢歌曲",
      "cmds": [
        "Like",
        "喜欢歌曲"
      ]
    },
    {
      "code": "music-up",
      "explain": "YesPlayMusic 音量加",
      "cmds": [
        "Volume Up",
        "音量加"
      ]
    },
    {
      "code": "music-down",
      "explain": "YesPlayMusic 音量减",
      "cmds": [
        "Volume Down",
        "音量减"
      ]
    },
    {
      "code": "music-repeat",
      "explain": "YesPlayMusic 开关循环播放",
      "cmds": [
        "Toggle Repeat",
        "开关循环播放"
      ]
    },
    {
      "code": "music-shuffle",
      "explain": "YesPlayMusic 开关随机播放",
      "cmds": [
        "Toggle Shuffle",
        "开关随机播放"
      ]
    },
    {
      "code": "music-lyric",
      "explain": "YesPlayMusic 开关桌面歌词",
      "cmds": [
        "Toggle Lyric",
        "开关歌词"
      ]
    },
    {
      "code": "music-list",
      "explain": "YesPlayMusic 所有命令",
      "cmds": [
        "YesPlayMusic All Commands",
        "YesPlayMusic 所有命令"
      ]
    }
  ]
}